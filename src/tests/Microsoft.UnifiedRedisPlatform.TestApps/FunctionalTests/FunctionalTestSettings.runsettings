    <!--Resource Time Away-->
    <Parameter name="FunctionalTest:PS-PreProd-01:Resource-Time-Away:Password" value="" />
    <Parameter name="FunctionalTest:PS-Prod-01:Resource-Time-Away:Password" value="" />

    <!--Global Resource Management-->
    <Parameter name="FunctionalTest:PS-PreProd-01:Global-Resource-Management-SIT:Password" value="" />
    <Parameter name="FunctionalTest:PS-PreProd-01:Global-Resource-Management-Aurora-SIT:Password" value="" />
