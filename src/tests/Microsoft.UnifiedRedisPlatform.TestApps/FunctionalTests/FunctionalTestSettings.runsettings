    <Parameter name="FunctionalTest:Regions" value="eastus,westus" />
    
    <!--FXP-Service-->
    <Parameter name="FunctionalTest:PS-PreProd-01:FXP-Service-SIT:Password" value="" />
    <Parameter name="FunctionalTest:PS-PreProd-01:FXP-Service:Password" value="" />
    <Parameter name="FunctionalTest:PS-Prod-01:FXP-Service:Password" value="" />

