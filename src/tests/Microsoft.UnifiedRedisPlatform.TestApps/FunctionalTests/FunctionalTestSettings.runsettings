    <Parameter name="FunctionalTest:TokenUri" value="api/token"/>
    <Parameter name="FunctionalTest:ConfigUri" value="api/configurations"/>
    
    <!--Clusters-->
    <Parameter name="FunctionalTest:Clusters" value="PS-PreProd-01,PS-Prod-01" />
